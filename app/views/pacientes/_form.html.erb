<%= simple_form_for(@paciente) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombres %>
    <%= f.input :apellidos %>
    <%= f.input :fecha_nacimiento %>
    <%= f.input :edad %>
    <%= f.input :sexo %>
    <%= f.input :direccion %>
    <%= f.input :ciudad %>
    <%= f.input :telefono %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

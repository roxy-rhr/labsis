<%= simple_form_for(@datos_usuario) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombres %>
    <%= f.input :apellidos %>
    <%= f.input :direccion %>
    <%= f.input :telefono %>
    <%= f.input :ci %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
